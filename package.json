{
  "name": "gamejam",
  "version": "1.0.0",
  "description": "Basic three.js engine bindings",
  "main": "index.js",
  "scripts": {
    "dev": "copyfiles index.html dist/client/ && webpack serve --config ./src/client/webpack.dev.js",
    "build": "webpack --config ./src/client/webpack.prod.js && copyfiles index.html dist/client/",
    "start": "node dist/server/server.js",
    "build-run": "npm run build && npm run start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "three": "^0.129.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/three": "^0.129.1",
    "ts-loader": "^9.2.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
